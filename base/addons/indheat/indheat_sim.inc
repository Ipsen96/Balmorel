INDHEAT_VHSTOVOLT.UP(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO,S,T)$(INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),INDHEAT_IAGKNY(Y,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO)))) =
 INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO);

