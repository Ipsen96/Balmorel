INDHEAT_VGH_T.UP(IY411,INDHEAT_IA,INDHEAT_DHUSER,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO,S,T)$(INDHEAT_GINGROUP(INDHEAT_IGHSTO,INDHEAT_IGROUPSTO) AND INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),INDHEAT_IAGKNY(Y,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO)))) =
 INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO)/INDHEAT_GDATA(INDHEAT_IGHSTO,'GDSTOHUNLD')*INDHEAT_GTODHUSER(INDHEAT_IGHSTO,INDHEAT_DHUSER);
***STORAGE LOADING***
INDHEAT_VHSTOLOADT.UP(IY411,INDHEAT_IA,INDHEAT_DHUSER,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO,S,T)$( INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),INDHEAT_IAGKNY(Y,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO)))) =
 INDHEAT_GKFX(IY411,INDHEAT_IA,INDHEAT_IGHSTO,INDHEAT_IGROUPSTO)/INDHEAT_GDATA(INDHEAT_IGHSTO,'GDSTOHLOAD')*INDHEAT_GTODHUSER(INDHEAT_IGHSTO,INDHEAT_DHUSER);
