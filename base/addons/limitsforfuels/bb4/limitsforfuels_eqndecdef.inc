Equations
   QLIMITSFORFUELS_GMAXCF(Y,C,FFF,IFFFALIAS,IFFFALIAS2)       'Maximum fuel usage per country for a shared max for three fuels'
;

QLIMITSFORFUELS_GMAXCF(IY411,C,LIMITSFORFUELS_NUMCONMAXC)$LIMITSFORFUELS_GMAXFSET(LIMITSFORFUELS_NUMCONMAXC,C,FFF)..
    SUM(FFF$(LIMITSFORFUELS_GMAXFSET(LIMITSFORFUELS_NUMCONMAXC,C,FFF)),IGMAXF(IY411,C,FFF)
    + VLIMITSFORFUELS_QGMAXCF(IY411,C,FFF,LIMITSFORFUELS_NUMCONMAXC) )
         =G=
    SUM(IA$ICA(C,IA),
     SUM(G$(IAGK_HASORPOT(IY411,IA,G) AND (GDATA(G,'GDFUEL') EQ FDATA(FFF,'FDACRONYM'))), IOF3P6 * SUM((IS3,T), IHOURSINST(IS3,T) * VGF_T(IY411,IA,G,IS3,T)))
    +SUM(G$(IAGK_HASORPOT(IY411,IA,G) AND (GDATA(G,'GDFUEL') EQ FDATA(IFFFALIAS,'FDACRONYM'))), IOF3P6 * SUM((IS3,T), IHOURSINST(IS3,T) * VGF_T(IY411,IA,G,IS3,T)))
    +SUM(G$(IAGK_HASORPOT(IY411,IA,G) AND (GDATA(G,'GDFUEL') EQ FDATA(IFFFALIAS2,'FDACRONYM'))), IOF3P6 * SUM((IS3,T), IHOURSINST(IS3,T) * VGF_T(IY411,IA,G,IS3,T)))
    );
