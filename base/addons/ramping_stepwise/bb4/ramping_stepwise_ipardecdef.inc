** RAMPING CHARACTERISTICS OF THE TECHNOLOGIES TO BE USED
PARAMETER IRAMPING_G_COSTS_UP(G,RAMPING_RATE) 'Ramping up costs for each technology (money/MWh)';
PARAMETER IRAMPING_G_COSTS_DOWN(G,RAMPING_RATE) 'Ramping down costs for each technology (money/MWh)';
PARAMETER IRAMPING_G_CAPABILITY_UP(G,RAMPING_RATE) 'Ramping up capability for each technology for each ramping rate mode (% Capacity)';
PARAMETER IRAMPING_G_CAPABILITY_DOWN(G,RAMPING_RATE) 'Ramping down capability for each technology for each ramping rate mode (% Capacity)';
LOOP (RAMPING_TECH_TYPE,
         LOOP (RAMPING_TECH_GROUP,
                 LOOP (RAMPING_FUELS,
                         LOOP ((G,RAMPING_RATE)$IRAMPING_G_RATE(G,RAMPING_RATE),
                                 if(((GDATA(G,'GDTYPE') = RAMPING_TECH_TYPE_VALUE (RAMPING_TECH_TYPE)) AND (GDATA(G,'GDTECHGROUP')=RAMPING_TECH_GROUP_VALUE(RAMPING_TECH_GROUP)) AND (GDATA(G,'GDFUEL') = RAMPING_FUELS_VALUE(RAMPING_FUELS))),
                                         IRAMPING_G_COSTS_UP(G,RAMPING_RATE)=RAMPING_COSTS_UP(RAMPING_TECH_TYPE,RAMPING_TECH_GROUP,RAMPING_FUELS,RAMPING_RATE);
                                         IRAMPING_G_COSTS_DOWN(G,RAMPING_RATE)=RAMPING_COSTS_DOWN(RAMPING_TECH_TYPE,RAMPING_TECH_GROUP,RAMPING_FUELS,RAMPING_RATE);
                                         IRAMPING_G_CAPABILITY_UP(G,RAMPING_RATE)=RAMPING_CAPABILITY_UP(RAMPING_TECH_TYPE,RAMPING_TECH_GROUP,RAMPING_FUELS,RAMPING_RATE);
                                         IRAMPING_G_CAPABILITY_DOWN(G,RAMPING_RATE)=RAMPING_CAPABILITY_DOWN(RAMPING_TECH_TYPE,RAMPING_TECH_GROUP,RAMPING_FUELS,RAMPING_RATE);
                                 );
                         );
                 );
         );
);
