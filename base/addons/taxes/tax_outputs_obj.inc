$ifi NOT %TAXES%==yes  $goto No_TAXES2
OBJ_YCR(Y,C,IR,'CONSUMER_TAXES','POWER_DH','Mmoney')$CCCRRR(C,IR)=
OMONEY/IOF1000000*(

* Tax on electricity consumption
    + SUM(DEUSER, TAX_DE(C,DEUSER) * SUM((S,T), IHOURSINST(S,T) * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0))  )

$ifi %DFLEXQUANT%==yes           + SUM((DEUSER), TAX_DE(C,DEUSER) *(
$ifi %DFLEXQUANT%==yes             SUM(DEF_U1,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_U1) ) - SUM(DEF_D1,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_D1) )
$ifi %DFLEXQUANT%==yes           + SUM(DEF_U2,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_U2) ) - SUM(DEF_D2,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_D2) )
$ifi %DFLEXQUANT%==yes           + SUM(DEF_U3,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_U3) ) - SUM(DEF_D3,VDEF_T.L(Y,IR,DEUSER,S,T,DEF_D3) ))


+SUM(IA$RRRAAA(IR,IA),
* Tax on heat consumption
    + SUM(DHUSER, TAX_DH(C,DHUSER) * DH(Y,IA,DHUSER))

$ifi %DFLEXQUANT%==yes      + SUM((DHUSER), TAX_DH(C,DHUSER) * (
$ifi %DFLEXQUANT%==yes          SUM(DHF_U1,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_U1) ) - SUM(DHF_D1,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_D1) )
$ifi %DFLEXQUANT%==yes        + SUM(DHF_U2,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_U2) ) - SUM(DHF_D2,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_D2) )
$ifi %DFLEXQUANT%==yes        + SUM(DHF_U3,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_U3) ) - SUM(DHF_D3,VDHF_T.L(Y,IA,DHUSER,S,T,DHF_D3) ) )
)
)
;
$label NO_TAXES2
