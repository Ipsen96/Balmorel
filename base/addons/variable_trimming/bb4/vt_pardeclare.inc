VT_VAREL(YYY,AAA,GGG) 'variable costs for el production'
VT_VARHEAT(YYY,AAA,GGG) 'variable costs for heat production'
;

SETS
VT_REQMIN(YYY,AAA,GGG) 'technologies with minimum usage requirements'
VT_REQFX(YYY,AAA,GGG) 'technologies with fixed usage requirements'
VT_REQMAX(YYY,AAA,GGG) 'technologies with maximum usage requirements'

VT_REQEMIN(YYY,AAA,GGG) 'technologies with minimum usage requirements'
