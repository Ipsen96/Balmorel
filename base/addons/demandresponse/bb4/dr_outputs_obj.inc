
*DEMANDRESPONSE
$ifi NOT %DEMANDRESPONSE%==yes  $goto No_DROBJ
OBJ_YCR(Y,C,IR,'DEMAND_RESPONSE_COSTS','POWER_DH','Mmoney')$CCCRRR(C,IR)=
OMONEY/IOF1000000*(

** Variable operation costs
    SUM((IA,DR_TECH)$(IADR(Y,IA,DR_TECH) AND RRRAAA(IR,IA)), DR_DATA(DR_TECH,'DROMVCOST') * SUM((S,T), IHOURSINST(S,T) * VDR_DOWN.L(Y,IA,DR_TECH,S,T)))

** Fixed operation costs for technologies with investment cost and without
  +  SUM((IA,DR_TECH)$(IADR(Y,IA,DR_TECH) AND DR_DATA(DR_TECH,'DROMFCOST') AND DR_DATA(DR_TECH,'DRINVCOST') AND RRRAAA(IR,IA)), DR_DATA(DR_TECH,'DROMFCOST')  * VDRKN_ACCUMNET.L(Y,IA,DR_TECH))
  +  SUM((IA,DR_TECH)$(IADR(Y,IA,DR_TECH) AND DR_DATA(DR_TECH,'DROMFCOST') AND NOT DR_DATA(DR_TECH,'DRINVCOST') AND RRRAAA(IR,IA)), DR_DATA(DR_TECH,'DROMFCOST')  * IDR_FX(Y,IA,DR_TECH))

** Investment costs
  +  SUM((IA,DR_TECH)$(IADR(Y,IA,DR_TECH) AND DR_DATA(DR_TECH,'DRINVCOST') AND RRRAAA(IR,IA)),ANNUITYDR(DR_TECH) * DR_DATA(DR_TECH,'DRINVCOST')  * VDRKN_ACCUMNET.L(Y,IA,DR_TECH))
);
$label No_DROBJ