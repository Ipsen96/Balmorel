VARIABLES
VMINLOAD(YYY,AAA,GGG,SSS,MINLOAD_DAY)
VMINLOAD_DAY(YYY,AAA,GGG,SSS,DAY)


EQUATION QMINLOAD_MAX(YYY,AAA,GGG,SSS,MINLOAD_DAY,TTT);
EQUATION QMINLOAD_MIN(YYY,AAA,GGG,SSS,MINLOAD_DAY,TTT);
EQUATION QMINLOAD_MAX_DAY(YYY,AAA,GGG,SSS,DAY,TTT);
EQUATION QMINLOAD_MIN_DAY(YYY,AAA,GGG,SSS,DAY,TTT);

QMINLOAD_MAX(IY411,IA,G,S,MINLOAD_DAY,T)$(IAGK_HASORPOT(IY411,IA,G) AND IS3(S) AND minload_techs(G) AND iminload_day(minload_day,T))..
         VGE_T(IY411,IA,G,S,T)
         =L=
         VMINLOAD(IY411,IA,G,S,MINLOAD_DAY) / MINLOAD(G);

QMINLOAD_MIN(IY411,IA,G,S,MINLOAD_DAY,T)$(IAGK_HASORPOT(IY411,IA,G) AND IS3(S) AND minload_techs(G) AND iminload_day(minload_day,T))..
         VGE_T(IY411,IA,G,S,T)
         =G=
         VMINLOAD(IY411,IA,G,S,MINLOAD_DAY);

QMINLOAD_MAX_DAY(IY411,IA,G,S,DAY,T)$(IAGK_HASORPOT(IY411,IA,G) AND IS3(S) AND minload_techs_DAY(G) AND IDAYT(DAY,T))..
         VGE_T(IY411,IA,G,S,T)
         =L=
         VMINLOAD_DAY(IY411,IA,G,S,DAY) / MINLOAD(G);

QMINLOAD_MIN_DAY(IY411,IA,G,S,DAY,T)$(IAGK_HASORPOT(IY411,IA,G) AND IS3(S) AND minload_techs_DAY(G) AND IDAYT(DAY,T))..
         VGE_T(IY411,IA,G,S,T)
         =G=
         VMINLOAD_DAY(IY411,IA,G,S,DAY);
