GR_FC_OUTPUT_TECH(Y,IY411,IA,IGETOH)$IY411FIRST(Y)  =

* Fixed component and subscribed power (per MW installed)

$ifi %BB4%==yes   IOF1_ *  (

                 + IOF1000 * SUM(IR, SUM(IAGK_HASORPOT(IY411,IA,IGETOH)$RRRAAA(IR,IA),

                         IGR_PRICE_TECH(IY411,IR,'GRDSUBS')  * (VGKNACCUMNET.L(IY411,IA,IGETOH) + GKFX(IY411,IA,IGETOH)) /(GDATA(IGETOH,'GDFE') *(1$(NOT GEFFRATE(IA,IGETOH))+GEFFRATE(IA,IGETOH)))     ))

);



GR_EC_OUTPUT_TECH(Y,IY411,IA,IGETOH)$IY411FIRST(Y)  =

$ifi %BB4%==yes  IOF1_ *  (

* Energy charge winter

                 + SUM(IGR_TIME('GRDECW',IR,IS3,T)$RRRAAA(IR,IA),   SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDECW')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

* Energy charge summer

                 + SUM(IGR_TIME('GRDECS',IR,IS3,T)$RRRAAA(IR,IA),   SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDECS')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

* Energy charge TOU step 1

                 + SUM(IGR_TIME('GRDTOU1',IR,IS3,T)$RRRAAA(IR,IA),  SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDTOU1')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

* Energy charge TOU step 2

                 + SUM(IGR_TIME('GRDTOU2',IR,IS3,T)$RRRAAA(IR,IA),  SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDTOU2')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

* Energy charge TOU step 3

                 + SUM(IGR_TIME('GRDTOU3',IR,IS3,T)$RRRAAA(IR,IA),  SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDTOU3')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

* Energy charge TOU step 4

                 + SUM(IGR_TIME('GRDTOU4',IR,IS3,T)$RRRAAA(IR,IA),  SUM((IAGK_HASORPOT(IY411,IA,IGETOH)),IHOURSINST(IS3,T) * IGR_PRICE_TECH(IY411,IR,'GRDTOU4')  * VGF_T.L(IY411,IA,IGETOH,IS3,T)))

);



GR_DC_OUTPUT_TECH(Y,IY411,IA)$IY411FIRST(Y) =

$ifi %BB4%==yes   IOF1_ *  (

* Demand charge winter

                 + IOF1000 * SUM((IR,MMM)$(RRRAAA(IR,IA) AND SUM((IS3,T)$IGR_TIME('GRDDCW',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VGETOH_CAP1.L(IY411,IA,MMM) *  IGR_PRICE_TECH(IY411,IR,'GRDDCW'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge winter

* Demand charge summer

                 + IOF1000 * SUM((IR,MMM)$(RRRAAA(IR,IA) AND SUM((IS3,T)$IGR_TIME('GRDDCS',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VGETOH_CAP1.L(IY411,IA,MMM) *  IGR_PRICE_TECH(IY411,IR,'GRDDCS'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge summer

* Demand charge 2 (e.g for spring and fall months)

                 + IOF1000 * SUM((IR,MMM)$(RRRAAA(IR,IA) AND SUM((IS3,T)$IGR_TIME('GRDDC2',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VGETOH_CAP1.L(IY411,IA,MMM) *  IGR_PRICE_TECH(IY411,IR,'GRDDC2'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge 2

* Demand charge TOU

                 + IOF1000 * SUM((IR,MMM)$RRRAAA(IR,IA),   VGETOH_CAP2.L(IY411,IA,MMM) *  IGR_PRICE_TECH(IY411,IR,'GRDDCTOU'))/(IGR_MONTHSINSIM/card(MMM))

* Demand charge yearly

                 + IOF1000 * SUM(IR$RRRAAA(IR,IA),   VGETOH_CAPY.L(IY411,IA) *  IGR_PRICE_TECH(IY411,IR,'GRDDCY'))

);



GR_EC_OUTPUT_USER(Y,IY411,IR,DEUSER)$IY411FIRST(Y) =

$ifi %BB4%==yes  IOF1_ *  (

* Tariffs on user groups

                 + SUM((IS3,T)$IGR_TIME('GRDECW',IR,IS3,T),   IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDECW')  * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )

                 + SUM((IS3,T)$IGR_TIME('GRDECS',IR,IS3,T),   IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDECS')  * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )

                 + SUM((IS3,T)$IGR_TIME('GRDTOU1',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU1') * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )

                 + SUM((IS3,T)$IGR_TIME('GRDTOU2',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU2') * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )

                 + SUM((IS3,T)$IGR_TIME('GRDTOU3',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU3') * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )

                 + SUM((IS3,T)$IGR_TIME('GRDTOU4',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU4') * ( ((DE(IY411,IR,DEUSER) * DE_VAR_T(IR,DEUSER,IS3,T)) / IDE_SUMST(IR,DEUSER))$(IDE_SUMST(IR,DEUSER) GT 0)) )



$ifi not %DemandResponse%==yes  $goto label_GridTariffs_postsolve_DR_END

                 + SUM((IS3,T)$IGR_TIME('GRDECW',IR,IS3,T),   SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDECS')  * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T))))

                 + SUM((IS3,T)$IGR_TIME('GRDECS',IR,IS3,T),   SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDECS')  * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T))))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU1',IR,IS3,T),  SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU1') * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T) )))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU2',IR,IS3,T),  SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU2') * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T))))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU3',IR,IS3,T),  SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU3') * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T) )))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU4',IR,IS3,T),  SUM((IA,DR_TECH)$(IADR(IY411,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDTOU4') * (VDR_UP.L(IY411,IA,DR_TECH,IS3,T) - VDR_DOWN.L(IY411,IA,DR_TECH,IS3,T) )))



$LABEL label_GridTariffs_postsolve_DR_END

);



$ifi not %EV%==yes  $goto label_GridTariffs_postsolve_EV_END

** EV's are assumed to be in the residential user group

GR_EC_OUTPUT_USER(Y,IY411,IR,'RESE')$IY411FIRST(Y) = GR_EC_OUTPUT_USER(Y,IY411,IR,'RESE')$IY411FIRST(Y) +

$ifi %BB4%==yes  IOF1_ *  (

                 + SUM((IS3,T)$IGR_TIME('GRDECW',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDECW')   * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

                 + SUM((IS3,T)$IGR_TIME('GRDECS',IR,IS3,T),  IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDECS')   * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU1',IR,IS3,T), IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDTOU1')  * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU2',IR,IS3,T), IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDTOU2')  * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU3',IR,IS3,T), IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDTOU3')  * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

                 + SUM((IS3,T)$IGR_TIME('GRDTOU4',IR,IS3,T), IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(IY411,IR,'RESE','GRDTOU4')  * (VEV_VNETCHARGE_BEV.L(IY411,IR,IS3,T) + VEV_VNETCHARGE_PHEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_BEV.L(IY411,IR,IS3,T) - VEV_VNETDISCHARGE_PHEV.L(IY411,IR,IS3,T)))

);

$LABEL label_GridTariffs_postsolve_EV_END





GR_DC_OUTPUT_USER(Y,IY411,IR,DEUSER)$IY411FIRST(Y) =

$ifi %BB4%==yes  IOF1_ *  (

                 + IOF1000 * SUM(MMM$(SUM((IS3,T)$IGR_TIME('GRDDCW',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VDEUSER_CAP1.L(IY411,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDDCW'))/(IGR_MONTHSINSIM/CARD(MMM))    !! Demand charge winter

                 + IOF1000 * SUM(MMM$(SUM((IS3,T)$IGR_TIME('GRDDCS',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VDEUSER_CAP1.L(IY411,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDDCS'))/(IGR_MONTHSINSIM/CARD(MMM))    !! Demand charge summer

                 + IOF1000 * SUM(MMM$(SUM((IS3,T)$IGR_TIME('GRDDC2',IR,IS3,T), ISSSTTTMMM(IS3,T,MMM))), VDEUSER_CAP1.L(IY411,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDDC2'))/(IGR_MONTHSINSIM/CARD(MMM))    !! Demand charge 2

                 + IOF1000 * SUM(MMM,   VDEUSER_CAP2.L(IY411,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDDCTOU'))/(IGR_MONTHSINSIM/card(MMM))

                 + IOF1000 * VDEUSER_CAPY.L(IY411,IR,DEUSER) *  IGR_PRICE_DEUSER(IY411,IR,DEUSER,'GRDDCY')

);


