                 + SUM((IGR_TIME('GRDTOU1',IR,IS3,T),DEUSER),  SUM((IA,DR_TECH)$(IADR(Y,IA,DR_TECH) AND RRRAAA(IR,IA) AND DR_TECH_USER(DR_TECH,DEUSER)),IHOURSINST(IS3,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDTOU1') * (VDR_UP(Y,IA,DR_TECH,IS3,T) - VDR_DOWN(Y,IA,DR_TECH,IS3,T) )))
