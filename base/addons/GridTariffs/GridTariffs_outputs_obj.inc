*GRID TARIFFS2
$ifi NOT %GRIDTARIFFS%==yes  $goto No_GRIDTARIFFS2OBJ
OBJ_YCR(Y,C,IR,'CONSUMER_GRID_TARIFFS','POWER_DH','Mmoney')$CCCRRR(C,IR)=
OMONEY/IOF1000000*(

* Demand charge winter
                 + IOF1000 * SUM((MMM)$(SUM((S,T)$IGR_TIME('GRDDCW',IR,S,T), ISSSTTTMMM(S,T,MMM))), SUM(IA$(SUM(IGETOH,IAGK_HASORPOT(Y,IA,IGETOH)) AND RRRAAA(IR,IA)), VGETOH_CAP1.L(Y,IA,MMM)) *  IGR_PRICE_TECH(Y,IR,'GRDDCW'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge winter
* Demand charge summer
                 + IOF1000 * SUM((MMM)$(SUM((S,T)$IGR_TIME('GRDDCS',IR,S,T), ISSSTTTMMM(S,T,MMM))), SUM(IA$(SUM(IGETOH,IAGK_HASORPOT(Y,IA,IGETOH)) AND RRRAAA(IR,IA)), VGETOH_CAP1.L(Y,IA,MMM)) *  IGR_PRICE_TECH(Y,IR,'GRDDCS'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge summer
* Demand charge 2 (e.g for spring and fall months)
                 + IOF1000 * SUM((MMM)$(SUM((S,T)$IGR_TIME('GRDDC2',IR,S,T), ISSSTTTMMM(S,T,MMM))), SUM(IA$(SUM(IGETOH,IAGK_HASORPOT(Y,IA,IGETOH)) AND RRRAAA(IR,IA)), VGETOH_CAP1.L(Y,IA,MMM)) *  IGR_PRICE_TECH(Y,IR,'GRDDC2'))/(IGR_MONTHSINSIM/card(MMM))    !! Demand charge 2

* Demand charge TOU
                 + IOF1000 * SUM((MMM),   SUM(IA$(SUM(IGETOH,IAGK_HASORPOT(Y,IA,IGETOH)) AND RRRAAA(IR,IA)), VGETOH_CAP2.L(Y,IA,MMM)) *  IGR_PRICE_TECH(Y,IR,'GRDDCTOU'))/(IGR_MONTHSINSIM/card(MMM))
* Demand charge yearly
                 + IOF1000 *    SUM(IA$(SUM(IGETOH,IAGK_HASORPOT(Y,IA,IGETOH)) AND RRRAAA(IR,IA)), VGETOH_CAPY.L(Y,IA)) *  IGR_PRICE_TECH(Y,IR,'GRDDCY')

* Tariffs on user groups
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDECW',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDECW')  * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDECS',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDECS')  * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDTOU1',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDTOU1') * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDTOU2',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDTOU2') * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDTOU3',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDTOU3') * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )
                 + SUM((S,T,DEUSER)$IGR_TIME('GRDTOU4',IR,S,T),  IHOURSINST(S,T) * IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDTOU4') * VDENET_T.L(Y,IR,DEUSER,S,T)$(IDE_SUMST(IR,DEUSER) GT 0) )

                 + IOF1000 * SUM((DEUSER,MMM)$(SUM((S,T)$IGR_TIME('GRDDCW',IR,S,T), ISSSTTTMMM(S,T,MMM))), VDEUSER_CAP1.L(Y,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDDCW'))/(IGR_MONTHSINSIM/CARD(MMM))
                 + IOF1000 * SUM((DEUSER,MMM)$(SUM((S,T)$IGR_TIME('GRDDCS',IR,S,T), ISSSTTTMMM(S,T,MMM))), VDEUSER_CAP1.L(Y,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDDCS'))/(IGR_MONTHSINSIM/CARD(MMM))
                 + IOF1000 * SUM((DEUSER,MMM)$(SUM((S,T)$IGR_TIME('GRDDC2',IR,S,T), ISSSTTTMMM(S,T,MMM))), VDEUSER_CAP1.L(Y,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDDC2'))/(IGR_MONTHSINSIM/CARD(MMM))
                 + IOF1000 * SUM((DEUSER,MMM),   VDEUSER_CAP2.L(Y,IR,DEUSER,MMM) *  IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDDCTOU'))/(IGR_MONTHSINSIM/card(MMM))
                 + IOF1000 * SUM((DEUSER),  VDEUSER_CAPY.L(Y,IR,DEUSER) *  IGR_PRICE_DEUSER(Y,IR,DEUSER,'GRDDCY'))

)
;
$label NO_GRIDTARIFFS2OBJ
